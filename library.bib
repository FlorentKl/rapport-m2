title = {{Efficient processing and analysis of large-scale light-sheet microscopy data}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26426501 http://www.nature.com/doifinder/10.1038/nprot.2015.111},
volume = {10},
year = {2015}
