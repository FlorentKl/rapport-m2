editor = {Novitch, Bennett G.},
file = {:C$\backslash$:/Users/Flore/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Cho et al. - 2014 - Isl1 Directly Controls a Cholinergic Neuronal Identity in the Developing Forebrain and Spinal Cord by Forming Cell T.pdf:pdf},
isbn = {10.1371/journal.pgen.1004280},
issn = {15537404},
journal = {PLoS Genet.},
