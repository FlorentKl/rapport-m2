file = {:C$\backslash$:/Users/Flore/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Stone, Nikolics - 1995 - Tissue- and age-specific expression patterns of alternatively spliced agrin mRNA transcripts in embryonic rat s.pdf:pdf},
