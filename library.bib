journal = {Development},
keywords = {Apoptosis,Chick,Foxp1,Hox,Motor neuron,Neck},
month = {dec},
number = {24},
pages = {4645--4657},
pmid = {26393773},
title = {{Motor neurons with limb-innervating character in the cervical spinal cord are sculpted by apoptosis based on the Hox code in chick embryo}},
