issn = {18758908},
journal = {J. Alzheimer's Dis.},
keywords = {Alzheimer's disease,Tg2576,dementia,hippocampus,mouse model,reference memory,transgenic,working memory},
month = {aug},
number = {1},
pages = {105--126},
pmid = {21558645},
title = {{Which memory task for my mouse? A systematic review of spatial memory performance in the Tg2576 alzheimer's mouse model}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21558645 http://www.medra.org/servlet/aliasResolver?alias=iospress{\&}doi=10.3233/JAD-2011-101827},
