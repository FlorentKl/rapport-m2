pmid = {23639831},
title = {{Neurotrophin and Wnt signaling cooperatively regulate dendritic spine formation}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1044743113000547 http://www.ncbi.nlm.nih.gov/pubmed/23639831 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3793870},
