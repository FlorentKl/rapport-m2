pmid = {29436205},
publisher = {Yonsei University College of Medicine},
title = {{FAT1 gene alteration in facioscapulohumeral muscular dystrophy type 1}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/29436205 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5823839},
