title = {{Wnt signaling: complexity at the surface}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12775774 http://jcs.biologists.org/cgi/doi/10.1242/jcs.00623 http://jcs.biologists.org/cgi/doi/10.1242/jcs.02826},
volume = {119},
