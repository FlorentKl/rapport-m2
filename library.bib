month = {may},
number = {5},
pages = {e2004734},
pmid = {29768404},
primaryClass = {10.1101},
publisher = {Public Library of Science},
title = {{Tissue-specific activities of the Fat1 cadherin cooperate to control neuromuscular morphogenesis}},
