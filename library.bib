@article{Wang2017,
abstract = {Stably Expressed Genes (SEGs) whose expression varies within a$\backslash$nnarrow range may be involved in core cellular processes$\backslash$nnecessary for basic functions. To identify such genes, we$\backslash$nre-analyzed existing RNA-Seq gene expression profiles across$\backslash$n11 organs at 4 developmental stages (from immature to old age)$\backslash$nin both sexes of F344 rats (n = 4/group; 320 samples).$\backslash$nExpression changes (calculated as the maximum expression /$\backslash$nminimum expression for each gene) of {\textgreater}19000 genes across$\backslash$norgans, ages, and sexes ranged from 2.35 to {\textgreater}109-fold, with a$\backslash$nmedian of 165-fold. The expression of 278 SEGs was found to$\backslash$nvary 4-fold and these genes were significantly involved$\backslash$nin protein catabolism (proteasome and ubiquitination), RNA$\backslash$ntransport, protein processing, and the spliceosome. Such$\backslash$nstability of expression was further validated in human samples$\backslash$nwhere the expression variability of the homologous human SEGs$\backslash$nwas significantly lower than that of other genes in the human$\backslash$ngenome. It was also found that the homologous human SEGs were$\backslash$ngenerally less subject to non-synonymous mutation than other$\backslash$ngenes, as would be expected of stably expressed genes. We also$\backslash$nfound that knockout of SEG homologs in mouse models was more$\backslash$nlikely to cause complete preweaning lethality than non-SEG$\backslash$nhomologs, corroborating the fundamental roles played by SEGs$\backslash$nin biological development. Such stably expressed genes and$\backslash$npathways across life-stages suggest that tight control of$\backslash$nthese processes is important in basic cellular functions and$\backslash$nthat perturbation by endogenous (e.g., genetics) or exogenous$\backslash$nagents (e.g., drugs, environmental factors) may cause serious$\backslash$nadverse effects.},
