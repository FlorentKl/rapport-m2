doi = {10.1016/B978-0-444-59565-2.00002-2},
file = {:C$\backslash$:/Users/Flore/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Romero, Mezmezian, Fidzia{\'{n}}ska - 2013 - Main steps of skeletal muscle development in the human. Morphological analysis and ultrastructura.pdf:pdf},
isbn = {9780444595652},
issn = {00729752},
pages = {1299--1310},
pmid = {23622355},
title = {{Main steps of skeletal muscle development in the human. Morphological analysis and ultrastructural characteristics of developing human muscle.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23622355 http://linkinghub.elsevier.com/retrieve/pii/B9780444595652000022},
