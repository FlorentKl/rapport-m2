}
@techreport{Klee2017,
abstract = {Astyanax mexicanus is a teleost fish originating in Central America (Mexico and USA). Two distinct forms exist within this species: the surface-dwelling form (SF) and the cave-dwelling form (CF), which lives in total darkness with really few interactions with outside world. The cave-dwelling form presents a lack of pigmentation and is blind, due to eye degeneration during early development. An eye will however develops, but exhibits morphological modifications before disappearing. My project was to study and compare the morphogenetical regionalization of the eye between SF and CF. This study was performed using in situ hybridization targeting marker genes of different retinal regions. I found that there is a naso-temporal rotation around the lens of these regions in the cave-dwelling form. This rotation may be due to an increase of Shh expression in the early CF embryo, which could make retinal cells adopt optic stalk fate, mainly in the nasal part of the eye which seems to be more sensitive. This change of fate would push away the other regions incompatible with this fate, and would be responsible for the coloboma-like phenotype of the cavefish eye.},
author = {Klee, Florent},
file = {:C$\backslash$:/Users/Flore/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Klee, Florent - 2017 - Etude de l'{\'{e}}volution de la morphogen{\`{e}}se et de la r{\'{e}}gionalisation de la r{\'{e}}gion optique chez Astyanax mexicanus.pdf:pdf},
