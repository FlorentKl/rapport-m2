pmid = {17507554},
publisher = {Society for Neuroscience},
title = {{Wnt-7a Induces Presynaptic Colocalization of 7-Nicotinic Acetylcholine Receptors and Adenomatous Polyposis Coli in Hippocampal Neurons}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12223543 http://www.jneurosci.org/cgi/doi/10.1523/JNEUROSCI.3934-06.2007},
volume = {27},
