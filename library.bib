isbn = {0012-1606 (Print)$\backslash$n0012-1606 (Linking)},
issn = {00121606},
journal = {Dev. Biol.},
keywords = {Apoptosis,Motor neuron,Programmed cell death},
month = {oct},
number = {1},
pages = {60--71},
pmid = {10491257},
publisher = {Academic Press},
title = {{Patterns of programmed cell death in populations of developing spinal motoneurons in chicken, mouse, and rat}},
url = {https://www.sciencedirect.com/science/article/pii/S0012160699994137 https://www.sciencedirect.com/science/article/pii/S0012160699994137?via{\%}3Dihub},
volume = {214},
