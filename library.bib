arxivId = {NIHMS150003},
author = {Alunni, Alessandro and Menuet, Arnaud and Candal, Eva and P{\'{e}}nigault, Jean Baptiste and Jeffery, William R and R{\'{e}}taux, Sylvie},
doi = {10.1002/cne.21488},
eprint = {NIHMS150003},
isbn = {1096-9861},
issn = {00219967},
journal = {J. Comp. Neurol.},
keywords = {Cell death,Evolution,Eye,Fgf8,Fish,GABA,LIM-homeodomain,Pax6,Proliferation,Shh,Teleost,Vax1},
month = {nov},
number = {2},
pages = {221--233},
