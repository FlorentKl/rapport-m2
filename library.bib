publisher = {Oxford University Press for The Company of Biologists Limited},
title = {{LDL-receptor-related protein 4 is crucial for formation of the neuromuscular junction}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17119023 http://dev.biologists.org/cgi/doi/10.1242/dev.02696},
volume = {133},
