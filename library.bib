issn = {22111247},
journal = {Cell Rep.},
keywords = {Hox gene,locomotion,motor neuron,neural development,sensory neuron,spinal circuit,transcription factor},
month = {oct},
number = {4},
pages = {867--877},
pmid = {29069594},
publisher = {NIH Public Access},
title = {{Columnar-Intrinsic Cues Shape Premotor Input Specificity in Locomotor Circuits}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/29069594 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5665584},
