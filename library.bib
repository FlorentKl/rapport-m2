file = {:Users/Etudiants/Library/Application Support/Mendeley Desktop/Downloaded/Moreno-Estell{\'{E}}s et al. - 2012 - Symmetric Expansion of Neural Stem Cells from the Adult Olfactory Bulb Is Driven by Astrocytes Via WNT7.pdf:pdf},
isbn = {3491822327},
