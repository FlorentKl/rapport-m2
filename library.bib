pmid = {23312954},
publisher = {Europe PMC Funders},
title = {{Wnt your brain be inflamed? Yes, it Wnt!}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23312954 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3595301},
volume = {19},
year = {2013}
}
@article{Jennings1993,
