isbn = {0953-816X (Print)},
issn = {0953816X},
journal = {Eur. J. Neurosci.},
keywords = {CaMKII,LRP,PDZ,PSD,PSD-95,SAP97},
month = {jun},
number = {11},
pages = {2864--2876},
pmid = {16819975},
title = {{Interaction of LDL receptor-related protein 4 (LRP4) with postsynaptic scaffold proteins via its C-terminal PDZ domain-binding motif, and its regulation by Ca2+/calmodulin-dependent protein kinase II}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16819975 http://doi.wiley.com/10.1111/j.1460-9568.2006.04846.x},
