issn = {0027-8424},
journal = {Proc. Natl. Acad. Sci. U. S. A.},
keywords = {adult hippocampus,autocrine Wnt signaling,dendrite arbors},
month = {jan},
number = {4},
pages = {E619--E628},
pmid = {28069946},
publisher = {National Academy of Sciences},
title = {{Wnt5a is essential for hippocampal dendritic maintenance and spatial learning and memory in adult mice}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28069946 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5278440 http://www.pnas.org/lookup/doi/10.1073/pnas.1615792114},
volume = {114},
