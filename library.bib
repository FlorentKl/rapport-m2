file = {:C$\backslash$:/Users/Flore/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Vilovi{\'{c}} et al. - 2006 - Cell death in developing human spinal cord.pdf:pdf},
issn = {0340-2061},
journal = {Anat. Embryol. (Berl).},
keywords = {Antigens,Apoptosis,Apoptosis: physiology,CD95,CD95: metabolism,Caspase 3,Caspase 3: metabolism,Cell Count,Humans,Immunohistochemistry,In Situ Nick-End Labeling,Neurons,Neurons: cytology,Neurons: enzymology,Neurons: metabolism,Proto-Oncogene Proteins c-bcl-2,Proto-Oncogene Proteins c-bcl-2: metabolism,Spinal Cord,Spinal Cord: cytology,Spinal Cord: embryology,Spinal Cord: enzymology,Spinal Cord: metabolism},
