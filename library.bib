pmid = {28376329},
publisher = {University Press, Cambridge},
title = {{The first European cave fish}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28376329 http://linkinghub.elsevier.com/retrieve/pii/S0960982217302191},
volume = {27},
year = {2017}
}
@article{Chang2018,
abstract = {Muscles control body movement and locomotion, posture and body position and soft tissue support. Mesoderm derived cells gives rise to 700 unique muscles in humans as a result of well-orchestrated signaling and transcriptional networks in specific time and space. Although the anatomical structure of skeletal muscles is similar, their functions and locations are specialized. This is the result of specific signaling as the embryo grows and cells migrate to form different structures and organs. As cells progress to their next state, they suppress current sequence specific transcription factors (SSTF) and construct new networks to establish new myogenic features. In this review, we provide an overview of signaling pathways and gene regulatory networks during formation of the craniofacial, cardiac, vascular, trunk, and limb skeletal muscles.},
