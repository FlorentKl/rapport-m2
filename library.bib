issn = {0950-1991},
journal = {Development},
keywords = {Dermomyotome,Embryonic stem cells,Muscle differentiation,Muscular dystrophy,Paraxial mesoderm,Pluripotent stem cells,Skeletal myogenesis,Somite,iPS cells},
