isbn = {1662-5099 (Electronic)$\backslash$r1662-5099 (Linking)},
issn = {1662-5099},
journal = {Front. Mol. Neurosci.},
keywords = {adults,cyclic AMP-responsive element-binding protein,hippocampus,mammals,memory,neurogenesis,neuronal plasticity,transcription factors},
pages = {46},
pmid = {26379491},
publisher = {Frontiers Media SA},
title = {{A new perspective on the role of the CREB family of transcription factors in memory consolidation via adult hippocampal neurogenesis}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26379491 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4549561 http://journal.frontiersin.org/Article/10.3389/fnmol.2015.00046/abstract},
