isbn = {1932-6203 (Electronic) 1932-6203 (Linking)},
issn = {19326203},
journal = {PLoS One},
number = {1},
pages = {e53553},
pmid = {23326453},
publisher = {Public Library of Science},
title = {{De Novo Sequencing of Astyanax mexicanus Surface Fish and Pach{\'{o}}n Cavefish Transcriptomes Reveals Enrichment of Mutations in Cavefish Putative Eye Genes}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23326453 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3541186},
volume = {8},
year = {2013}
}
@article{Banerjee2011,
abstract = {Trunk neural crest cells delaminate from the dorsal neural tube as an uninterrupted sheet; however, they convert into segmentally organized streams before migrating through the somitic territory. These neural crest cell streams join the segmental trajectories of pathfinding spinal motor axons, suggesting that interactions between these two cell types might be important for neural crest cell migration. Here, we show that in the zebrafish embryo migration of both neural crest cells and motor axons is temporally synchronized and spatially restricted to the center of the somite, but that motor axons are dispensable for segmental neural crest cell migration. Instead, we find that muscle-specific receptor kinase (MuSK) and its putative ligand Wnt11r are crucial for restricting neural crest cell migration to the center of each somite. Moreover, we find that blocking planar cell polarity (PCP) signaling in somitic muscle cells also results in non-segmental neural crest cell migration. Using an F-actin biosensor we show that in the absence of MuSK neural crest cells fail to retract non-productive leading edges, resulting in non-segmental migration. Finally, we show that MuSK knockout mice display similar neural crest cell migration defects, suggesting a novel, evolutionarily conserved role for MuSK in neural crest migration. We propose that a Wnt11r-MuSK dependent, PCP-like pathway restricts neural crest cells to their segmental path.},
author = {Banerjee, Santanu and Gordon, Laura R and Donn, Thomas M and Berti, Caterina and Moens, Cecilia B and Burden, Steven J. and Granato, Michael},
doi = {10.1242/dev.067306},
isbn = {0950-1991},
issn = {0950-1991},
journal = {Development},
month = {aug},
number = {15},
pages = {3287--3296},
pmid = {21750038},
publisher = {Company of Biologists},
title = {{A novel role for MuSK and non-canonical Wnt signaling during segmental neural crest cell migration}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21750038 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3133918 http://dev.biologists.org/cgi/doi/10.1242/dev.067306},
volume = {138},
year = {2011}
}
@article{Sanes1999,
abstract = {We describe the formation, maturation, elimination, maintenance, and regeneration of vertebrate neuromuscular junctions (NMJs), the best studied of all synapses. The NMJ forms in a series of steps that involve the exchange of signals among its three cellular components--nerve terminal, muscle fiber, and Schwann cell. Although essentially any motor axon can form NMJs with any muscle fiber, an additional set of cues biases synapse formation in favor of appropriate partners. The NMJ is functional at birth but undergoes numerous alterations postnatally. One step in maturation is the elimination of excess inputs, a competitive process in which the muscle is an intermediary. Once elimination is complete, the NMJ is maintained stably in a dynamic equilibrium that can be perturbed to initiate remodeling. NMJs regenerate following damage to nerve or muscle, but this process differs in fundamental ways from embryonic synaptogenesis. Finally, we consider the extent to which the NMJ is a suitable model for development of neuron-neuron synapses.},
author = {Sanes, Joshua R. and Lichtman, Jeff W.},
doi = {10.1146/annurev.neuro.22.1.389},
file = {:Users/Etudiants/Library/Application Support/Mendeley Desktop/Downloaded/Sanes, Lichtman - 1999 - DEVELOPMENT OF THE VERTEBRATE NEUROMUSCULAR JUNCTION.pdf:pdf},
isbn = {0147-006X},
issn = {0147-006X},
journal = {Annu. Rev. Neurosci.},
keywords = {acetylcholine receptor,regeneration,synapse elimination,synapse formation},
month = {mar},
number = {1},
pages = {389--442},
pmid = {10202544},
publisher = {Annual Reviews 4139 El Camino Way, P.O. Box 10139, Palo Alto, CA 94303-0139, USA},
title = {{Development of the Vertebrate Neuromascular Junction}},
url = {http://www.annualreviews.org/doi/10.1146/annurev.neuro.22.1.389 http://www.annualreviews.org/doi/full/10.1146/annurev.neuro.22.1.389?url{\_}ver=Z39.88-2003{\&}rfr{\_}id=ori{\%}253Arid{\%}253Acrossref.org{\&}rfr{\_}dat=cr{\_}pub{\%}253Dpubmed},
