isbn = {1662-5102},
issn = {1662-5102},
journal = {Front. Cell. Neurosci.},
keywords = {VEGF,amyotrophic lateral sclerosis,motor neurons,neurotrophic factors,spinal cord neurodegeneration},
pages = {61},
pmid = {24616665},
publisher = {Frontiers Media SA},
title = {{Trophic factors as modulators of motor neuron physiology and survival: implications for ALS therapy}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24616665 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3937589 http://journal.frontiersin.org/article/10.3389/fncel.2014.00061/abstract},
