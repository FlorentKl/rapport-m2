journal = {Neurosci. Res.},
keywords = {Astrocyte,Cytokine expression,In vitro,Wnt ligand,Wnt receptor,Wnt regulator,Wnt signaling},
month = {jan},
pages = {16--29},
pmid = {27562517},
publisher = {Elsevier},
title = {{Analysis of the expression of the Wnt family of proteins and its modulatory role on cytokine expression in non activated and activated astroglial cells}},
url = {https://www.sciencedirect.com/science/article/pii/S0168010216301134?via{\%}3Dihub},
volume = {114},
