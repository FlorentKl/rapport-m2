pages = {347--353},
pmid = {9418991},
publisher = {Wiley-Blackwell},
title = {{Total number and size distribution of motor neurons in the spinal cord of normal and EMC-virus infected mice--a stereological study.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9418991 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC1467692},
