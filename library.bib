isbn = {1552-4833 (Electronic)$\backslash$r1552-4825 (Linking)},
issn = {15524833},
journal = {Am. J. Med. Genet. Part A},
keywords = {Cardiovascular disease,Development,Heart},
month = {jun},
number = {6},
pages = {1347--1371},
pmid = {23633400},
title = {{Development of the human heart}},
