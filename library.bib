title = {{Myogenesis in the mouse embryo: Differential onset of expression of myogenic proteins and the involvement of titin in myofibril assembly}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/2474551 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2115733},
volume = {109},
