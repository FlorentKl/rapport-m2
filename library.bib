pages = {4217--4229},
pmid = {26525673},
title = {{Wnt/Notum spatial feedback inhibition controls neoblast differentiation to regulate reversible growth of the planarian brain}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26525673 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4689217 http://dev.biologists.org/cgi/doi/10.1242/dev.123612},
