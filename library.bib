title = {{Biglycan Is an Extracellular MuSK Binding Protein Important for Synapse Stability}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22396407 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3313673 http://www.jneurosci.org/cgi/doi/10.1523/JNEUROSCI.4610-11.2012},
