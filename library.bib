pages = {15857--15866},
pmid = {19332546},
publisher = {American Society for Biochemistry and Molecular Biology},
title = {{Wnt-5a/JNK signaling promotes the clustering of PSD-95 in hippocampal neurons}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19332546 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2708882},
volume = {284},
