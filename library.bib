title = {{Left–right dissociation of hippocampal memory processes in mice}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/1542680 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC48548 http://www.pnas.org/lookup/doi/10.1073/pnas.1405648111},
volume = {111},
