journal = {J. Clin. Invest.},
month = {may},
number = {5},
pages = {2268--2286},
pmid = {23585476},
publisher = {American Society for Clinical Investigation},
title = {{WNT signaling underlies the pathogenesis of neuropathic pain in rodents}},
url = {http://www.jci.org http://www.ncbi.nlm.nih.gov/pubmed/23585476 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3635721},
volume = {123},
year = {2013}
}
@article{Miller2002,
abstract = {SUMMARY The Wnt genes encode a large family of secreted protein growth factors that have been identified in animals from hydra to humans. In humans, 19 WNT proteins have been identified that share 27{\%} to 83{\%} amino-acid sequence identity and a conserved pattern of 23 or 24 cysteine residues. Wnt genes are highly conserved between vertebrate species sharing overall sequence identity and gene structure, and are slightly less conserved between vertebrates and invertebrates. During development, Wnts have diverse roles in governing cell fate, proliferation, migration, polarity, and death. In adults, Wnts function in homeostasis, and inappropriate activation of the Wnt pathway is implicated in a variety of cancers.},
