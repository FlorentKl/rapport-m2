isbn = {1744987107084},
issn = {14697580},
keywords = {3D-PDF,Anatomy,Embryology,Morphogenesis,Muscle development,Ontogeny,Three-dimensional reconstruction},
month = {may},
pmid = {29726018},
title = {{Hitherto unknown detailed muscle anatomy in an 8-week-old embryo}},
