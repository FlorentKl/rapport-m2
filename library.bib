file = {:Users/Etudiants/Library/Application Support/Mendeley Desktop/Downloaded/Leung et al. - 2016 - WNT$\beta$-catenin signaling mediates human neural crest induction via a pre-neural border intermediate.pdf:pdf},
isbn = {9788578110796},
issn = {0950-1991},
journal = {Development},
keywords = {Embryonic stem cells,Human,Neural border,Neural crest,$\beta$-Catenin signaling},
month = {feb},
number = {3},
pages = {398--410},
pmid = {26839343},
publisher = {Company of Biologists},
title = {{WNT/ -catenin signaling mediates human neural crest induction via a pre-neural border intermediate}},
