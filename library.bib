pmid = {22833130},
publisher = {Company of Biologists},
title = {{Onecut transcription factors act upstream of Isl1 to regulate spinal motoneuron diversification}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22833130 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4074251 http://dev.biologists.org/cgi/doi/10.1242/dev.078501},
