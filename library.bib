file = {:C$\backslash$:/Users/Flore/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Cuitino et al. - 2010 - Wnt-5a modulates recycling of functional GABAA receptors on hippocampal neurons.pdf:pdf},
isbn = {0270-6474},
issn = {0270-6474},
journal = {J. Neurosci.},
month = {jun},
number = {25},
pages = {8411--8420},
pmid = {20573888},
publisher = {Society for Neuroscience},
title = {{Wnt-5a Modulates Recycling of Functional GABAA Receptors on Hippocampal Neurons}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20573888 http://www.jneurosci.org/cgi/doi/10.1523/JNEUROSCI.5736-09.2010},
