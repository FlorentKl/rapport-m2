pages = {6545--6557},
pmid = {19458225},
publisher = {Society for Neuroscience},
title = {{Interruption of -Catenin Signaling Reduces Neurogenesis in Alzheimer's Disease}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19458225 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3618977 http://www.jneurosci.org/cgi/doi/10.1523/JNEUROSCI.0421-09.2009},
