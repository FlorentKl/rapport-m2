publisher = { Mary Ann Liebert, Inc.  140 Huguenot Street, 3rd Floor New Rochelle, NY 10801 USA  },
title = {{Wnt-Ryk Signaling Mediates Axon Growth Inhibition and Limits Functional Recovery after Spinal Cord Injury}},
url = {https://www.liebertpub.com/doi/pdfplus/10.1089/neu.2008.0776 http://www.liebertonline.com/doi/abs/10.1089/neu.2008.0776},
