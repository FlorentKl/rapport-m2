pages = {1245--1260},
pmid = {23826406},
publisher = {e-Century Publishing Corporation},
title = {{Expression of Wnt5a and its receptor Fzd2 is changed in the spinal cord of adult amyotrophic lateral sclerosis transgenic mice}},
