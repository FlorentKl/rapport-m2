pmid = {19925885},
title = {{Identification and characterisation of the developmental expression pattern of tbx5b, a novel tbx5 gene in zebrafish}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1567133X0900132X http://www.ncbi.nlm.nih.gov/pubmed/19925885},
volume = {10},
