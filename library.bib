isbn = {1750-2799 (Electronic)$\backslash$r1750-2799 (Linking)},
issn = {17542189},
journal = {Nat. Protoc.},
month = {oct},
number = {11},
pages = {1983--1995},
pmid = {23060243},
title = {{Three-dimensional imaging of solvent-cleared organs using 3DISCO}},
url = {http://www.nature.com/doifinder/10.1038/nprot.2012.119},
volume = {7},
year = {2012}
}
@article{Mardones2016,
abstract = {BACKGROUND In the adult hippocampus new neurons are continuously generated from neural stem cells (NSCs) present at the subgranular zone of the dentate gyrus. This process is controlled by Wnt signaling, which plays a complex role in regulating multiple steps of neurogenesis including maintenance, proliferation and differentiation of progenitor cells and the development of newborn neurons. Differential effects of Wnt signaling during progression of neurogenesis could be mediated by cell-type specific expression of Wnt receptors. Here we studied the potential role of Frizzled-1 (FZD1) receptor in adult hippocampal neurogenesis. RESULTS In the adult dentate gyrus, we determined that FZD1 is highly expressed in NSCs, neural progenitors and immature neurons. Accordingly, FZD1 is expressed in cultured adult hippocampal progenitors isolated from mouse brain. To evaluate the role of this receptor in vivo we targeted FZD1 in newborn cells using retroviral-mediated RNA interference. FZD1 knockdown resulted in a marked decrease in the differentiation of newborn cells into neurons and increased the generation of astrocytes, suggesting a regulatory role for the receptor in cell fate commitment. In addition, FZD1 knockdown induced an extended migration of adult-born neurons within the granule cell layer. However, no differences were observed in total dendritic length and dendritic arbor complexity between control and FZD1-deficient newborn neurons. CONCLUSIONS Our results show that FZD1 regulates specific stages of adult hippocampal neurogenesis, being required for neuronal differentiation and positioning of newborn neurons into the granule cell layer, but not for morphological development of adult-born granule neurons.},
author = {Mardones, Muriel D and Andaur, Gabriela A and Varas-Godoy, Manuel and Henriquez, Jenny F and Salech, Felipe and Behrens, Mar{\'{i}}a Isabel and Couve, Andr{\'{e}}s and Inestrosa, Nibaldo C. and Varela-Nallar, Lorena},
doi = {10.1186/s13041-016-0209-3},
file = {:C$\backslash$:/Users/Flore/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mardones et al. - 2016 - Frizzled-1 receptor regulates adult hippocampal neurogenesis.pdf:pdf},
issn = {17566606},
journal = {Mol. Brain},
keywords = {Adult neurogenesis,Frizzled,Hippocampus,Neural progenitor cells,Neuronal differentiation,Wnt signaling},
month = {mar},
number = {1},
pages = {29},
pmid = {26980182},
publisher = {BioMed Central},
title = {{Frizzled-1 receptor regulates adult hippocampal neurogenesis}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26980182 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4791773},
volume = {9},
year = {2016}
}
@article{Retaux2013a,
abstract = {Within the species Astyanax mexicanus, there are several inter-fertile populations of river-dwelling sighted fish and cave-dwelling blind fish which have evolved morphological and behavioral adaptations. We have recently reported a developmental and neurophysiological basis for the loss of aggressive behavior in the blind cavefish morph of Astyanax. Using an appropriate behavioral assay, we have shown that surface Astyanax show intense dominance-related aggressiveness. The expression of this behavior is inversely correlated with the serotonin (5HT) levels in their hindbrain raphe nucleus. Moreover this behavior is not solely visually-evoked and has a genetic component. Conversely in cavefish, there is no raphe-driven dominance aggressiveness. Instead, the embryonic Sonic Hedgehog-dependent modification of the size of a serotonergic neuronal group localized in their hypothalamus causes a shift in their behavioral pattern: instead of fighting, they search for food. Here we further discuss the origin and nature of this behavioral shift.},
author = {R{\'{e}}taux, Sylvie and Elipot, Yannick},
doi = {10.4161/cib.23166},
isbn = {1942-0889},
issn = {1942-0889},
journal = {Commun. Integr. Biol.},
keywords = {aggressive,astyanax mexicanus,behaviour,brain development,evo-,foraging,hedgehog,lution,serotonin,social behaviour,sonic},
month = {mar},
number = {April},
pages = {e23166},
pmid = {23749249},
publisher = {Taylor {\&} Francis},
title = {{Feed or fight: A behavioral shift in blind cavefish.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23749249 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3609845 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3609845{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {6},
